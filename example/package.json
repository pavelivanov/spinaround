{
  "name": "spinonediv",
  "version": "0.0.1",
  "description": "A collection of single element css loaders and spinners",
  "homepage": "https://github.com/grammka/spinonediv#readme",
  "author": "Pavel Ivanov (grammka@gmail.com)",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "source": "node ./util/source.js",
    "html": "jade ./src -o ./build -P -w",
    "css": "stylus -w ./src/style.styl -o ./build -u ../node_modules/nib/lib/nib --import ./node_modules/normalize.styl/_normalize.styl",
    "build": "npm run clean && jade ./src/style.styl -o ./build -P && stylus ./src -o ./build -u ../node_modules/nib/lib/nib --import ./node_modules/normalize.styl/_normalize.styl"
  },
  "dependencies": {
    "normalize.styl": "^3.0.3"
  },
  "devDependencies": {
    "glob": "^7.1.1",
    "stylus": "^0.54.5"
  }
}
